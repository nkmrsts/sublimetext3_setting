{
    "env": {
        "browser": true,
        "jquery":  true,
        "amd":     true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "indent": [
            "error",
            "tab", {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "eqeqeq": [
            "error"
        ],
        "no-useless-computed-key": [
            "error"
        ],
        "no-duplicate-imports": [
            "error"
        ],
        "arrow-spacing": [
            "error", {
                "before": true,
                "after": true
            }
        ],
        "no-empty": [
            "error", {
                "allowEmptyCatch": true
            }
        ]
    },
    "globals": {
    }
}
